@using SW_PROJECT.Models;
@model SW_PROJECT.Models.Departments
@{
    ViewBag.Title = "EditDepartment";
}


    <div class="row">
        <div class=" col-lg-12">
            <h3 class="page-header">Edit Department</h3>
            <hr />

            <form class="form-signin" action="/Department/EditDepartment" method="post">

                <div class="hidden">
                    <div class="form-group">
                        <input type="text" name="ID" value="@Model.ID" />

                    </div>

                </div>

                <div class="form-group">
                    <label for="exampleInputEmail1">Department</label>
                    <input type="text" class="form-control" name="DeptName" value="@Model.DeptName">

                </div>



                <button type="submit" class="btn btn-primary">Save</button>
            </form>

        </div>

    </div>


